<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%t] %-5relative %-5level %logger{35} - %msg%n</pattern>
        </encoder>
    </appender>

<!--    <logger name="com.github.henkexbg" level="debug" additivity="false">-->
<!--        <appender-ref ref="STDOUT" />-->
<!--    </logger>-->

    <root level="info" additivity="false">
        <appender-ref ref="STDOUT" />
    </root>

</configuration>